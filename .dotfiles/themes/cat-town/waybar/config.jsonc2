    "pulseaudio": {
        "format": "<span font='Font Awesome 5 Free 11'>{icon:2}</span> <span font-family='0xProto'> {volume:4}% - {desc}</span>",
        "format-bluetooth": "<span font='Font Awesome 5 Free 11'>Ôäî {icon:2}</span> <span font-family='0xProto'>{volume:4}% - {desc}</span>",
        "format-alt": "<span font='Font Awesome 5 Free 11'>{icon:2}</span> <span font-family='0xProto'> {volume:4}%</span>",
        "format-alt-click": "click-right",
        "format-muted": "<span font='Font Awesome 5 Free 11'>Ôö©</span>",
        "format-icons": {
            "headphone": ["ÔÄ• ÔÄ¶", "ÔÄ• ÔÄß", "ÔÄ• ÔÄß", "ÔÄ• ÔÄ®"],
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄß", "ÔÄ®"]
        },
        "scroll-step": 2,
        "on-click": "pavucontrol",
        "tooltip": false
    },
    "custom/storage": {
        "format": "{} ÔÇ†",
        "format-alt": "{percentage}% ÔÇ†",
        "format-alt-click": "click-right",
        "return-type": "json",
        "interval": 60,
        "exec": "~/.config/waybar/modules/storage.sh"
    },
    "backlight": {
        "format": "{icon}",
        "format-alt": "{percent}% {icon}",
        "format-alt-click": "click-right",
        "format-icons": ["ÔÉ´", "Ôô≤"],
        "on-scroll-down": "light -A 1",
        "on-scroll-up": "light -U 1"
    },
    "temperature": {
        "format": "<span font='Font Awesome 5 Free 9'>Ôù´</span> {temperatureC:3}¬∞C",
        "hwmon-path": "/sys/class/thermal/thermal_zone11/temp",
        // "hwmon-path": "/sys/class/hwmon/hwmon7/temp1_input",
        // "hwmon-path": "/sys/devices/platform/coretemp.0/hwmon/hwmon6/temp1_input",
        "on-click": "terminator -e 'watch -n1 sensors'"
    },
    "custom/temperature": {
        // "return-type": "json",
        "exec": "sensors -j|jq '.\"coretemp-isa-0000\".\"Package id 0\".temp1_input'",
        "format": "<span font='Font Awesome 5 Free 9'>{icon}</span>  {}",
        "format-icons": ["üå°"],
        "interval": 5,
        "on-click": "terminator -e 'watch -n1 sensors'"
    },
    "custom/weather": {
        "format": "{}",
        "format-alt": "{alt}: {}",
        "format-alt-click": "click-right",
        "interval": 1800,
        "return-type": "json",
        "exec": "~/.config/waybar/modules/weather.sh",
        "exec-if": "ping wttr.in -c1",
        // "on-click": "terminator -p Small -e 'bash -c \"curl -s https://en.wttr.in; read line\"'"
        "on-click": "gnome-weather"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-alt": "{icon} idle {status}",
        "format-alt-click": "click-right",
        "format-icons": {
            "activated": "<span font='Font Awesome 5 Free 9'>ÔèÅ</span>",
            "deactivated": "<span font='Font Awesome 5 Free 9'>ÔÄ£</span>"
        },
        "tooltip": false
    },

    "tray": {
        "icon-size": 18,
        "spacing": 18
    },
    "wlr/taskbar": {
        "format": "{icon}",
        "icon-size": 14,
        "icon-theme": "Flat-Remix-Violet-Dark",
        "tooltip-format": "{title}",
        "on-click": "activate",
        "on-click-middle": "close"
    },
    "custom/notification": {
        "tooltip": false,
        "format": " {} {icon} ",
        "format-icons": {
          "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
          "none": "ÔÇ¢",
          "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
          "dnd-none": "Ôá∑",
          "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
          "inhibited-none": "ÔÇ¢",
          "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
          "dnd-inhibited-none": "Ôá∑"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
      },
    "custom/spotify": {
    "format": "{}",
    "return-type": "json",
    "on-click": "playerctl -p spotify play-pause",
    "on-click-right": "spotifatius toggle-liked",
    "on-click-middle": "playerctl -p spotify next",
    "exec": "spotifatius monitor"
  },
  "wlr/workspaces": {
    "on-click": "activate",
    "format": "<span font='0xProto'>{name}</span>",
    "all-outputs": true,
    "disable-scroll": false,
    "active-only": false
  },
  "tray": {
    "show-passive-items": true,
    "spacing": 10
  },
  "clock#simpleclock": {
    "tooltip": false,
    "format": "ÔÄó <span font-family='0xProto'> {:%H:%M}</span>"
  },
  "clock": {
    "format": "ÔÅ≥  <span font-family='0xProto'> {:L%a %d %b}</span>",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
  },
  "cpu": {
    "format": "Ôãõ  <span font-family='0xProto'>{usage}%</span>",
    "tooltip": true,
    "interval": 1
  },
  "memory": {
    "format": "<span font='Font Awesome 5 Free 9'>Ôî∏</span> <span font-family='0xProto'> {used:0.2f}G</span>"
  },
  "pulseaudio": {
    "format": "<span font='Font Awesome 5 Free 9'>{icon}</span> <span font-family='0xProto'>{volume}%</span>",
    "format-muted": "<span font='Font Awesome 5 Free 9'>Ôë¶</span> <span font-family='0xProto'> muted</span>",
    "format-icons": {
      "headphone": "ÔÄ• ",
      "hands-free": "Û∞Çë",
      "headset": "Û∞Çë",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
    },
    "on-click": "pavucontrol"
  },
    "custom/sep": {
    "format": "|",
    "tooltip": false
  },
  "custom/power": {
    "tooltip": false,
    "on-click": "wlogout -p layer-shell &",
    "format": "‚èª"
  }
}
