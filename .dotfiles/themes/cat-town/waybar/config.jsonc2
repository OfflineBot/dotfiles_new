{
  "layer": "top",
  "position": "top",
  "height": 32,
  "spacing": 4,
  "exclusive": true,
  "gtk-layer-shell": true,
  "passthrough": false,
  "fixed-center": true,
  "modules-left": ["hyprland/workspaces", "hyprland/window"],
  "modules-center": [],
  "modules-right": [
    "cpu",
    "memory",
    "pulseaudio",
    "clock",
    "clock#simpleclock",
    "tray",
    "battery",
    "network",
    "custom/power"
  ],


    "battery": {
        "format": "<span font='Font Awesome 5 Free 11'>{icon}</span> <span font-family='0xProto'> {capacity}%</span>",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"],
        "format-time": "{H}h{M}m",
        "format-charging": "<span font='Font Awesome 5 Free'>ÔÉß</span>  <span font='Font Awesome 5 Free 11'>{icon}</span> <span font-family='0xProto'> {capacity}%</span>",
        "format-full": "<span font='Font Awesome 5 Free'>ÔÉß</span>  <span font='Font Awesome 5 Free 11'>{icon}</span>  <span font-family='0xProto'>Charged</span>",
        "interval": 30,
        "states": {
            "warning": 25,
            "critical": 10
        },
        "tooltip": true,
        "tooltip-format": "Time left: {time}, Capacity: {capacity}%",
        "on-click": "3"
    },
    "network": {
        "format-wifi": "<span font='Font Awesome 5 Free 9'>Ôá´</span>",
        "format-ethernet": "{ifname}ÔÉÅ ",
        "format-ethernet": "{ifname} ÔÉÅ",
        "format-disconnected": "ÔÑß",
        "max-length": 50,
        "tooltip": true,
        "tooltip-format": "Connected: {essid} ({signalStrength}%)"
        /*"on-click": "kitty -e 'nmtui'" */
    },

    
    "sway/workspaces": {
        "disable-scroll": true,
        "disable-markup": false,
        "format": "<span font-family='0xProto'>{icon}</span>",
        "format-icons": {
            "1": "1 <span font='0xProto'> Ôâ©</span>",
            "2": "2 <span font='0xProto'> ÔÑ†</span>",
            "3": "3 <span font='0xProto'> ÔÑ°</span>",
            "4": "4 <span font='0xProto'> ÔÉ†</span>",
            "5": "5 <span font='0xProto'> ÔÜò</span>",
            "6": "6 <span font='0xProto'> ÔÜº</span>"
        }
    },
    "sway/window": {
        "max-length": 60,
        "tooltip": false
    },
    "clock": {
        "format": "<span font-family='0xProto'>{:%a %d %b - %H:%M}</span>",
        "on-click": "gnome-calendar"
    },
    "hyprland/window": {
        "format": "<span font-family='0xProto'>{}</span>",
        "separate-outputs": true
    },
    "custom/cpu_speed": {
        "interval": 10,
        "return-type": "json",
        "exec": "~/.config/waybar/modules/cpu_speed.sh",
        "format": "<span font='Font Awesome 5 Free 11'>{icon}</span>  <span font-family='0xProto'>{}</span>",
        "format-icons": ["Ôãõ"],
        "escape": true,
        "on-click": "terminator -e htop"
    },
    "sway/language": {
        "format": "<span font='Font Awesome 5 Free 9'>üè≥Ô∏è</span> {}",
        "tooltip-format": "{long}",
        "on-click": "swaymsg input 'type:keyboard' xkb_switch_layout next"
    },
    "custom/powermenu": {
        "return-type": "json",
        "exec": "~/.config/waybar/modules/powermenu.sh",
        "format": "<span font='Font Awesome 5 Free 9'>{icon}</span>  {}",
        "format-icons": ["ÔÄë"],
        "interval": 3600,
        "escape": true,
        "on-click": "~/sway/scripts/powermenu"
    },
    "custom/recorder": {
		"format": "ÔÄΩ",
		"return-type": "json",
		"interval": 4,
		"exec": "echo '{\"class\": \"recording\"}'",
		"exec-if": "pgrep wf-recorder"
	},
